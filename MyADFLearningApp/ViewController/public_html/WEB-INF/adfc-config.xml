<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="HomePage">
    <page>/HomePage.jspx</page>
  </view>
  <task-flow-call id="Edit-Registration">
    <task-flow-reference>
      <document>/WEB-INF/Edit-Registration.xml</document>
      <id>Edit-Registration</id>
    </task-flow-reference>
    <input-parameter>
      <name>mode</name>
      <value>#{requestScope.mode}</value>
    </input-parameter>
    <input-parameter>
      <name>regToEdit</name>
      <value>#{requestScope.regToEdit}</value>
    </input-parameter>
    <return-value>
      <name>regToEdit</name>
      <value>#{pageFlowScope.regToEdit}</value>
    </return-value>
  </task-flow-call>
  <view id="CatalogeCategoryPage">
    <page>/CatalogeCategoryPage.jspx</page>
  </view>
  <method-call id="CreateNewLine">
    <method>#{bindings.CreateInsert.execute}</method>
    <outcome>
      <fixed-outcome>CreateInsert</fixed-outcome>
    </outcome>
  </method-call>
  <method-call id="initilizeNewRegLines">
    <method>#{bindings.initilizeNewRegLines.execute}</method>
    <outcome>
      <fixed-outcome>initilizeNewRegLines</fixed-outcome>
    </outcome>
  </method-call>
  <view id="ItemDetailsPage">
    <page>/ItemDetailsPage.jspx</page>
  </view>
  <method-call id="ExecuteRegLines">
    <method>#{bindings.Execute.execute}</method>
    <outcome>
      <fixed-outcome>Execute</fixed-outcome>
    </outcome>
  </method-call>
  <view id="ItemListPage">
    <page>/ItemListPage.jspx</page>
  </view>
  <control-flow-rule>
    <from-activity-id>HomePage</from-activity-id>
    <control-flow-case>
      <from-outcome>NewOrEdit</from-outcome>
      <to-activity-id>Edit-Registration</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>Edit-Registration</from-activity-id>
    <control-flow-case>
      <from-outcome>CatalogPage</from-outcome>
      <to-activity-id>CatalogeCategoryPage</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>HomePage</from-outcome>
      <to-activity-id>HomePage</to-activity-id>
    </control-flow-case>
    <control-flow-case>
      <from-outcome>ItemListPage</from-outcome>
      <to-activity-id>ItemListPage</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>CatalogeCategoryPage</from-activity-id>
    <control-flow-case>
      <from-outcome>CreateNewLine</from-outcome>
      <to-activity-id>CreateNewLine</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>initilizeNewRegLines</from-activity-id>
    <control-flow-case>
      <from-outcome>initilizeNewRegLines</from-outcome>
      <to-activity-id>ItemDetailsPage</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>CreateNewLine</from-activity-id>
    <control-flow-case>
      <from-outcome>CreateInsert</from-outcome>
      <to-activity-id>initilizeNewRegLines</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>ItemDetailsPage</from-activity-id>
    <control-flow-case>
      <from-outcome>SaveRegLine</from-outcome>
      <to-activity-id>ExecuteRegLines</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>ItemListPage</from-activity-id>
    <control-flow-case>
      <from-outcome>CreateNewLine</from-outcome>
      <to-activity-id>CreateNewLine</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule>
    <from-activity-id>ExecuteRegLines</from-activity-id>
    <control-flow-case>
      <from-outcome>Execute</from-outcome>
      <to-activity-id>Edit-Registration</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean>
    <managed-bean-name>RegistrationBean</managed-bean-name>
    <managed-bean-class>view.beans.RegistrationBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>disableAddItemBtn</property-name>
      <value>false</value>
    </managed-property>
    <managed-property>
      <property-name>disableSearchCataBtn</property-name>
      <value>fasle</value>
    </managed-property>
  </managed-bean>
</adfc-config>
